.projectManageTip{
  background: rgb(253, 124, 124);
  color: white;
  font-size:24rpx;
  line-height: 40rpx;
  text-align: center;
}
.listScroll{
  height: 100%;
}
/* .list{
  padding:50rpx 10rpx;
} */
.listItem{
  padding:30rpx;
  line-height: 40rpx;
  background: #cef; 
  border:white solid 1px;
  margin: 0 10rpx;
  margin-top: 30rpx; 
}
.type_p{
  background: #cef !important; 
}
.type_t{
  background: lightgreen !important; 
}
.type_w{
  background: pink !important; 
}
.listItem .textContent{
  display: inline-block;
  width: 90%;
  line-height: 60rpx;
}
.listItem .itemLabel{
  float: left;
  display: inline-block;
  width: 30%;
}
.listItem .itemText{
  display: inline-block;
  width: 70%;
  font-weight: bold;
  word-wrap: break-word;
}
.listItem .itemShortLabel{
  float: left;
  display: inline-block;
  width: 20%;
}
.listItem .itemShortText{
  display: inline-block;
  float: left;
  width: 30%;
  font-weight: bold;
  word-wrap: break-word;
}
.notPublish{
  color: #666;
}
.published{
  color:darkblue;
}
.scoring{
  color:orange
}
.done{
  color:green;
}
.progress{
  width: 60%;
  line-height:40rpx;
  float:left;
  margin: 10rpx;
}
.progressText{
  position:absolute;
  display:inline-block;
  left:48%;
  margin-top:-20rpx;
  color: orange;
  font-weight: bold;
}
/* 一键复制 */
.listItem .copy{
  height:100%;
  position:relative;
  top:0;
  width:10%;
  float:right;
}
.listItem .copy image{
  width: 60rpx;
  height: 60rpx;
}
.listItem .copy text{
  height:20px;
  line-height:20px;
  text-align:center;
  float: left;
}
/* 添加评分项目 */
.addProject{
  padding:30rpx;
  color:white;
  line-height:60rpx;
  background:#59e;
  position:fixed;
  right:20rpx;
  bottom:120rpx;
  width:180rpx;
  height:180rpx;
  border-radius:90rpx;
}